{
    "name": "source-map-lib",
    "version": "0.0.1",
    "description": "A library for working with source maps with an emphasis on convenience and ease of use.",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "scripts": {
        "start": "nodemon",
        "start:dev": "nodemon",
        "clean": "rm -rf build/*",
        "c": "npm run compile",
        "compile": "tsc --incremental",
        "cw": "npm run compile:watch",
        "compile:watch": "tsc  --incremental --watch",
        "t": "npm run test",
        "test": "jest --runInBand",
        "tw": "npm run test:watch",
        "test:watch": "jest --watch --runInBand"
    },
    "keywords": [],
    "author": "ashley@codecapers.com.au",
    "license": "MIT",
    "dependencies": {
        "moment": "^2.29.1",
        "vlq": "^2.0.4",
        "merge-source-map": "^1.1.0"
    },
    "devDependencies": {
        "@types/jest": "^27.5.1",
        "@types/node": "^16.11.7",
        "jest": "^28.1.0",
        "nodemon": "^2.0.12",
        "source-map-support": "0.5.19",
        "ts-jest": "^28.0.2",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.4"
    }
}
