{
    "name": "evaluation-engine",
    "version": "1.0.0",
    "description": "A code library that provides code evaluation for Data-Forge Notebook",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "scripts": {
        "start": "node ./build/index.js",
        "s": "npm start",
        "start:dev": "nodemon",
        "sd": "npm run start:dev",
        "clean": "rm -rf build tsconfig.tsbuildinfo",
        "c": "npm run compile",
        "compile": "tsc --incremental",
        "cw": "npm run compile:watch",
        "compile:watch": "tsc  --incremental --watch --preserveWatchOutput",
        "t": "npm run test",
        "test": "jest",
        "tw": "npm run test:watch",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.10.2",
        "@babel/types": "7.9.5",
        "@codecapers/fusion": "1.0.15",
        "axios": "^0.18.0",
        "babel-traverse": "^6.26.0",
        "babel-types": "^6.26.0",
        "fs-extra": "^6.0.1",
        "gunzip-maybe": "^1.4.2",
        "lodash": "^4.17.10",
        "model": "workspace:*",
        "resolve-cwd": "^2.0.0",
        "semver": "^7.5.4",
        "source-map-lib": "workspace:*",
        "tar-stream": "^3.1.6",
        "typy": "^3.3.0",
        "utils": "workspace:*"
    },
    "devDependencies": {
        "@types/babel-traverse": "^6.25.5",
        "@types/babel-types": "^7.0.7",
        "@types/babel__core": "7.1.7",
        "@types/fs-extra": "^5.0.4",
        "@types/gunzip-maybe": "^1.4.2",
        "@types/jest": "^27.5.1",
        "@types/lodash": "^4.14.150",
        "@types/node": "^16.11.7",
        "@types/semver": "^7.5.6",
        "@types/tar-stream": "^3.1.3",
        "jest": "^28.1.0",
        "nodemon": "^2.0.16",
        "ts-jest": "^28.0.2",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.4"
    }
}
