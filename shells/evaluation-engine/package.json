{
    "name": "evaluation-engine-shell",
    "version": "1.0.0",
    "description": "The evaluation engine HTTP server, evaluates code for Data-Forge Notebook.",
    "scripts": {
        "start": "node ./build/index.js",
        "s": "npm start",
        "start:dev": "nodemon",
        "sd": "npm run start:dev",
        "clean": "rm -rf build/*",
        "c": "npm run compile",
        "compile": "tsc --incremental",
        "cw": "npm run compile:watch",
        "compile:watch": "tsc  --incremental --watch",
        "t": "npm run test",
        "test": "jest",
        "tw": "npm run test:watch",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "body-parser": "^1.20.0",
        "cors": "^2.8.5",
        "evaluation-engine": "workspace:*",
        "express": "5.0.0-beta.1",
        "fs-extra": "^10.1.0",
        "model": "workspace:*",
        "utils": "workspace:*"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "^27.5.1",
        "@types/node": "^16.11.7",
        "jest": "^28.1.0",
        "nodemon": "^2.0.16",
        "ts-jest": "^28.0.2",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.4"
    }
}
