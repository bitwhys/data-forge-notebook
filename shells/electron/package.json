{
    "name": "electron",
    "scripts": {
        "start": "pnpm run electron:static",
        "start:dev": "pnpm run electron:dev",
        "electron:static": "pnpm run build && pnpm run run-electron-static",
        "electron:dev": "pnpm run compile && concurrently --names=\"serve,electron\" --kill-others \"pnpm run serve\" \"pnpm run run-electron-dev\"",
        "build": "pnpm run compile && npm run pack",
        "run-electron-static": "cross-env ENTRY=../../dist/electron/index.html cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true electron build/main.js",
        "run-electron-dev": "cross-env ENTRY=http://localhost:8080/index.html cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true electron  build/main.js",
        "clean": "rm -rf ./dist build tsconfig.tsbuildinfo",
        "compile": "tsc --incremental",
        "compile:watch": "tsc --incremental --watch",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch --passWithNoTests",
        "serve": "webpack serve --config webpack.config.js",
        "pack": "webpack --config webpack.config.js",
        "s": "pnpm run start",
        "b": "pnpm run build",
        "c": "pnpm run compile",
        "cw": "pnpm run compile:watch",
        "sd": "pnpm run electron:dev",
        "t": "pnpm run test",
        "tw": "pnpm run test:watch"
    },
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "@storybook/addon-actions": "^6.4.19",
        "@storybook/addon-docs": "^6.4.19",
        "@storybook/addon-essentials": "^6.4.19",
        "@storybook/addon-interactions": "^6.4.19",
        "@storybook/addon-links": "^6.4.19",
        "@storybook/builder-webpack5": "^6.4.19",
        "@storybook/manager-webpack5": "^6.4.19",
        "@storybook/react": "^6.4.19",
        "@storybook/testing-library": "^0.0.9",
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/react": "^12.1.4",
        "@types/chai": "4.0.4",
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "^27.4.1",
        "@types/lodash": "^4.14.179",
        "@types/marked": "^4.0.3",
        "@types/node": "^16.11.7",
        "@types/react": "^17.0.39",
        "@types/react-beautiful-dnd": "^13.1.2",
        "@types/react-dom": "^17.0.13",
        "@types/styled-components": "^5.1.25",
        "@types/uuid": "^8.3.4",
        "babel-loader": "^8.2.3",
        "concurrently": "^7.0.0",
        "copy-webpack-plugin": "^10.2.4",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "electron": "^17.1.1",
        "file-loader": "^6.2.0",
        "fork-ts-checker-notifier-webpack-plugin": "^6.0.0",
        "fork-ts-checker-webpack-plugin": "^7.2.1",
        "fs-extra": "^10.1.0",
        "jest": "^27.5.1",
        "minimist": "^1.2.6",
        "msw": "^0.39.1",
        "source-map-loader": "^3.0.1",
        "style-loader": "^3.3.1",
        "ts-jest": "^27.1.3",
        "ts-loader": "^9.2.7",
        "typescript": "^4.6.2",
