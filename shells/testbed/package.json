{
    "name": "testbed",
    "scripts": {
        "start": "pnpm run serve",
        "clean": "rm -rf ./dist build tsconfig.tsbuildinfo",
        "compile": "tsc --incremental",
        "compile:watch": "tsc --incremental --watch",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch --passWithNoTests",
        "serve": "webpack serve --open index.html --config webpack.config.js",
        "s": "pnpm run start",
        "c": "pnpm run compile",
        "cw": "pnpm run compile:watch",
        "t": "pnpm run test",
        "tw": "pnpm run test:watch"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/react": "^12.1.4",
        "@types/jest": "^27.4.1",
        "@types/lodash": "^4.14.179",
        "@types/marked": "^4.0.3",
        "@types/node": "^16.11.7",
        "@types/react": "^17.0.39",
        "@types/react-beautiful-dnd": "^13.1.2",
        "@types/react-dom": "^17.0.13",
        "@types/uuid": "^8.3.4",
        "concurrently": "^7.0.0",
        "cross-env": "^7.0.3",
        "electron": "^17.1.1",
        "fs-extra": "^10.1.0",
        "jest": "^27.5.1",
        "msw": "^0.39.1",
        "ts-jest": "^27.1.3",
        "typescript": "^4.6.2"
    },
    "dependencies": {
        "@blueprintjs/core": "^3.10.0",
        "@blueprintjs/icons": "^3.4.0",
        "@codecapers/fusion": "1.0.15",
        "classnames": "^2.3.1",
        "humanize-duration": "^3.27.1",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "monaco-editor": "^0.32.1",
        "normalize.css": "^8.0.0",
        "path-browserify": "^1.0.1",
        "re-resizable": "^6.9.9",
        "react": "^17.0.2",
        "react-beautiful-dnd": "^13.1.0",
        "react-dom": "^17.0.2",
        "react-markdown": "^8.0.3",
        "uuid": "^8.3.2",
        "notebook-editor": "workspace:*",
        "model": "workspace:*"
    }
