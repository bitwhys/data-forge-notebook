{
    "name": "testbed",
    "scripts": {
        "start": "pnpm run serve",
        "clean": "rm -rf ./dist build tsconfig.tsbuildinfo",
        "compile": "tsc --incremental",
        "compile:watch": "tsc --incremental --watch --preserveWatchOutput",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch --passWithNoTests",
        "serve": "webpack serve --open index.html --config webpack.config.js",
        "s": "pnpm run start",
        "c": "pnpm run compile",
        "cw": "pnpm run compile:watch",
        "t": "pnpm run test",
        "tw": "pnpm run test:watch"
    },
    "devDependencies": {
        "@types/jest": "^27.4.1",
        "@types/node": "^16.11.7",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.13",
        "jest": "^27.5.1",
        "ts-jest": "^27.1.3",
        "typescript": "^4.6.2"
    },
    "dependencies": {
        "@codecapers/fusion": "1.0.15",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "notebook-editor": "workspace:*",
        "model": "workspace:*"
    }
}