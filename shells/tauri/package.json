{
    "name": "tauri",
    "scripts": {
        "start": "pnpm run tauri:dev",
        "start:dev": "pnpm run tauri:dev",
        "tauri:dev": "concurrently --names=\"serve,tauri\" --kill-others \"pnpm run serve\" \"pnpm run run-tauri-dev\"",
        "build": "cargo tauri build",
        "build-debug": "cargo tauri build --debug",
        "run-tauri-dev": "cargo tauri dev",
        "clean": "rm -rf ./dist build tsconfig.tsbuildinfo",
        "compile": "tsc --incremental",
        "compile:watch": "tsc --incremental --watch",
        "test": "jest",
        "test:watch": "jest --watch",
        "serve": "webpack serve --config webpack.config.js",
        "pack": "webpack --config webpack.config.js",
        "s": "pnpm run start",
        "b": "pnpm run build",
        "c": "pnpm run compile",
        "cw": "pnpm run compile:watch",
        "sd": "pnpm run tauri:dev",
        "t": "pnpm run test",
        "tw": "pnpm run test:watch"
    },
    "devDependencies": {
        "@types/minimist": "^1.2.2",
        "@types/jest": "28.1.6",
        "@types/node": "^16.11.7",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.13",
        "concurrently": "^7.0.0",
        "cross-env": "^7.0.3",
        "jest": "28.1.3",
        "ts-jest": "28.0.7",
        "typescript": "^4.6.2",
        "wait-on": "6.0.1"
    },
    "dependencies": {
        "minimist": "^1.2.5",
        "@codecapers/fusion": "1.0.15",
        "model": "workspace:*",
        "storage": "workspace:*",
        "notebook-editor": "workspace:*",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "utils": "workspace:*"
    }}