            "height": 102
        },
        {
            "id": "301b3f20-f31b-11ea-bfbc-fb3fa6416ecf",
            "cellType": "code",
            "cellScope": "global",
            "code": "const schedule = \"0 * * * * *\"; // This schedule runs once per minute.",
