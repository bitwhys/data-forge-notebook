{
  "name": "data-forge-notebook-editor-core",
  "version": "0.0.0",
  "description": "Data-Forge Notebook Editor-core.",
  "author": "ashley@codecapers.com.au",
  "main": "ts-build/electron-main.js",
  "scripts": {
    "start": "webpack serve --open browser.html --config webpack.browser.config.js",
    "s": "npm run start",
    "testbed": "webpack serve --open testbed.html --config webpack.testbed.config.js",
    "tb": "npm run testbed",
    "bb": "npm run build-browser",
    "build-browser": "webpack --config webpack.browser.config.js",
    "be": "npm run build-electron",
    "serve-electron": "webpack serve --config webpack.electron.config.js",
    "build-electron": "webpack --config webpack.electron.config.js",
    "storybook": "start-storybook -p 6006",
    "sb": "npm run storybook",
    "build-storybook": "build-storybook",
    "clean": "rm -rf ./dist ts-build tsconfig.tsbuildinfo",
    "compile": "tsc --incremental",
    "electron": "concurrently --names=\"build main,build browser\" \"npm run compile\" \"npm run build-electron\" && cross-env ENTRY=dist/electron/electron.html cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true electron .",
    "e": "npm run electron",
    "electron:live": "npm run compile && concurrently --names=\"serve,electron\" --kill-others \"npm run serve-electron\" \"cross-env ENTRY=http://localhost:8080/electron.html cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true electron .\"",
    "el": "npm run electron:live"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-docs": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "electron": "^17.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-notifier-webpack-plugin": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.7",
    "typescript": "^4.6.2",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@codecapers/fusion": "^1.0.14",
    "axios": "^0.25.0",
    "monaco-editor": "^0.32.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
