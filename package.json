{
  "name": "data-forge-notebook-editor-core",
  "description": "Data-Forge Notebook Editor-core.",
  "author": "ashley@codecapers.com.au",
  "main": "ts-build/index.js",
  "scripts": {
    "serve-browser": "webpack serve --open browser.html --config webpack.browser.config.js",
    "start": "pnpm run compile-plugins && concurrently \"pnpm run serve-plugins\" \"pnpm run serve-browser\" --kill-others",
    "s": "npm run start",
    "testbed": "webpack serve --open testbed.html --config webpack.testbed.config.js",
    "tb": "npm run testbed",
    "build-browser": "webpack --config webpack.browser.config.js",
    "bb": "npm run build-browser",
    "build": "pnpm run compile-plugins && pnpm run build-plugins && pnpm run build-browser",
    "b": "pnpm run build",
    "serve-electron": "webpack serve --config webpack.electron.config.js",
    "build-electron": "pnpm run compile-plugins && pnpm run build-plugins && webpack --config webpack.electron.config.js",
    "be": "npm run build-electron",
    "storybook": "start-storybook -p 6006",
    "sb": "npm run storybook",
    "build-storybook": "build-storybook",
    "clean": "pnpm run clean-plugins && rm -rf ./src/testbed/servces/plugins .parcel-cache ./dist ts-build tsconfig.tsbuildinfo",
    "compile": "pnpm run compile-plugins && tsc --incremental",
    "c": "npm run compile",
    "compile:watch": "tsc --incremental --watch",
    "cw": "npm run compile:watch",
    "electron": "concurrently --names=\"build main,build electron\" \"npm run compile\" \"npm run build-electron\" && cross-env ENTRY=../../dist/electron/electron.html cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true electron ts-build/testbed/electron-main.js",
    "e": "npm run electron",
    "electron:dev": "npm run compile && concurrently --names=\"serve-plugins,serve,electron\" --kill-others \"pnpm run serve-plugins\" \"npm run serve-electron\" \"cross-env ENTRY=http://localhost:8080/electron.html cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true electron  ts-build/testbed/electron-main.js\"",
    "ed": "npm run electron:dev",
    "test": "jest",
    "t": "npm run test",
    "test:watch": "jest --watch",
    "tw": "npm run test:watch",
    "serve-plugins": "node ./scripts/serve-plugins.js",
    "clean-plugins": "pnpm -r run clean",
    "compile-plugins": "pnpm -r run compile",
    "build-plugins": "node ./scripts/build-plugins.js"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/chai": "4.0.4",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/marked": "^4.0.3",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.39",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.13",
    "@types/styled-components": "^5.1.25",
    "@types/uuid": "^8.3.4",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^17.1.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-notifier-webpack-plugin": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "fs-extra": "^10.1.0",
    "jest": "^27.5.1",
    "minimist": "^1.2.6",
    "msw": "^0.39.1",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.7",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.10.0",
    "@blueprintjs/icons": "^3.4.0",
    "@codecapers/fusion": "1.0.15",
    "chai": "4.1.2",
    "classnames": "^2.3.1",
    "humanize-duration": "^3.27.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "monaco-editor": "^0.32.1",
    "normalize.css": "^8.0.0",
    "path-browserify": "^1.0.1",
    "re-resizable": "^6.9.9",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-markdown": "^8.0.3",
    "styled-components": "^5.3.5",
    "uuid": "^8.3.2",
    "host-bridge": "workspace:*"
  }
}
