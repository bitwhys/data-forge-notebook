{
  "name": "data",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start:dev": "parcel --log-level=none --host=127.0.0.1 --port=5001 index.html",
    "sd": "npm run start:dev",
    "start:test": "parcel --host=127.0.0.1 --port=5001 test.html",
    "st": "npm run start:test",
    "start": "npm run build && live-server out --cors --port=5001 --no-browser",
    "s": "npm run start",
    "build": "parcel build index.html --log-level=none --public-url . --no-source-maps && npm run module-hack && npm run create-output-directory && inliner dist/index.html > out/index.html",
    "b": "npm run build",
    "tsc": "tsc",
    "clean": "rm -rf dist out/* .parcel-cache",
    "create-output-directory": "node create-output-dir.js",
    "module-hack": "node module-hack.js"
  },
  "keywords": [],
  "author": "ashley@codecapers.com.au",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-tree": "^0.16.1",
    "host-bridge": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "fs-extra": "^10.1.0",
    "inliner": "^1.13.1",
    "live-server": "^1.2.1",
    "parcel": "^2.3.1",
    "typescript": "^4.5.2"
  }
}
